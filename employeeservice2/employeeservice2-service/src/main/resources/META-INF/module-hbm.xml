<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-lazy="false">
	<import class="com.employee.model.Employee" />
	<class name="com.employee.model.impl.EmployeeImpl" table="employee">
		<id name="eid" type="long">
			<generator class="increment" />
		</id>
		<property name="ename" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="emailid" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="mobile" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>