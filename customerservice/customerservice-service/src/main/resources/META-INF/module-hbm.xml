<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-lazy="false">
	<import class="com.customer.model.Customer" />
	<class name="com.customer.model.impl.CustomerImpl" table="customer">
		<id name="cid" type="long">
			<generator class="increment" />
		</id>
		<property name="cusername" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="cpass" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="cname" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="cemail" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="cmobile" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>